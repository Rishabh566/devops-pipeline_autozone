option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "autozone.settings"
    "PYTHONPATH": "/opt/python/current/app/autozone:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: AutoPlaza/autozone/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    StaticMappings: '[{"url":"/static/","path":"static"}]'
